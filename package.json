{
  "name": "@userdocks/react-sdk",
  "version": "0.2.0",
  "description": "The React SDK for userdocks. Securly store, access, and refresh access tokens in your React app.",
  "source": "src/index.ts",
  "main": "./dist/index.js",
  "module": "dist/module.js",
  "types": "./dist/index.d.ts",
  "private": false,
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "jest --coverage",
    "build": "parcel build",
    "watch": "parcel watch",
    "cleanup": "rm -rf ./node_modules && rm -rf ./dist && rm -rf ./.parcel-cache && rm -rf ./.cache && npm i",
    "prepublishOnly": "npm run cleanup && npm test && npm run build"
  },
  "author": "userdocks",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/userdocks/react-sdk"
  },
  "bugs": {
    "url": "https://github.com/userdocks/react-sdk/issues"
  },
  "homepage": "https://github.com/userdocks/react-sdk",
  "keywords": [
    "userdocks",
    "react",
    "sdk"
  ],
  "devDependencies": {
    "@parcel/packager-ts": "^2.3.2",
    "@parcel/transformer-typescript-types": "^2.3.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.0",
    "@types/react": "17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@userdocks/web-client-sdk": "^0.1.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.4.7",
    "parcel": "^2.3.2",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@userdocks/web-client-sdk": ">= 0.1.0",
    "react": ">= 16.8.0"
  }
}
