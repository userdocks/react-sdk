{
  "compilerOptions": {
    "target": "ES5",
    "lib": [
      "ES2017",
      "ES7",
      "ES6",
      "DOM"
    ],
    "jsx": "react-jsx",
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [
    "./node_modules",
    "./dist",
  ]
}
